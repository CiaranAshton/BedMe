<% provide(:title, "Add Property") %>
<%= form_for(@property) do |f| %>
  <%= f.label :addressLine1 %>
  <%= f.text_field :addressLine1 %>
  
  <%= f.label :addressLine2 %>
  <%= f.text_field :addressLine2 %>
  
  <%= f.label :town %>
  <%= f.text_field :town %>
  
  <%= f.label :country %>
  <%= f.text_field :country %>
  
  <%= f.label :postcode %>
  <%= f.text_field :postcode %>
  
   <%= f.label "Image" %>
  <%= f.file_field :img, multiple: false %>
  
  <%= f.submit "Add Property", class: "btn btn-primary" %>
<% end %>